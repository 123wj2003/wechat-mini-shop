.container{
    display: flex;
    flex:1;
    flex-direction: column;
}
.item{
    margin-top:5px;
    overflow:hidden;
    display:block;

}
.item:last-child{
    margin-bottom: 10px;
}
.list-empty{
    width: 100vw;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    flex:1;
    height: 100%;
}
.list-empty image{
    display: block;
    overflow: hidden;
    width: 122px;
    height: 122px;
}
.list-empty text{
    display: block;
    overflow: hidden;
    text-align: center;
    font-size: 14px;
    color: #999999;
}
