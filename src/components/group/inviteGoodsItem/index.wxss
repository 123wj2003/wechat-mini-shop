.page-goods-list-rows{
    overflow: hidden;
    display: block;
}
.page-goods-list-rows section{
    display:block;
    box-sizing:border-box;
}
.page-goods-list-rows section details{
    background-color: #fff;
    display: block;
    overflow: hidden;
    float: left;
    padding: 15px 15px 0 15px;
    position: relative;
    height: 90px;
    border-bottom: 1px solid #F8F8F8;
}
.page-goods-list-rows section details>image{
    margin:0;
    padding:0;
    border:0;
    display:inline-block;
    width:75px;
    height:75px;
    float:left;
    position:absolute;
}
.page-goods-list-rows section details .title-price{
    background-color:#fff;
    display:inline-block;
    overflow:hidden;
    float:left;
    margin-left:90px;
    position: relative;
    height: 100%;
}
.page-goods-list-rows section details:last-child .title-price{
    border-bottom: none;
}
.page-goods-list-rows section details .title-price .desc{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.page-goods-list-rows section details .title-price>text{
    color: #333333;
    height: 40px;
    line-height: 20px;
    font-size: 14px;
    overflow: hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;
}
.page-goods-list-rows section details .num-price{
    background-color: #fff;
    overflow: hidden;
    display: flex;
    align-items: center;
}

.page-goods-list-rows section details .num-price text{
    font-size: 14px;
    color: #FF635C;
    font-weight: bold;
}
.page-goods-list-rows section details .num-price i{
    font-size: 12px;
    color: #cacaca;
    margin-left: 10px;
    text-decoration:line-through
}

.page-goods-list-rows section details .group-num{
    background-color: #fff;
    overflow: hidden;
    display: flex;
    align-items: center;
    color: #999999;
}
.page-goods-list-rows section details .group-num text{
    font-size: 12px;
    margin-left: 5px;
}
.page-goods-list-rows section details .group-num i{
    font-size: 12px;
}
